package components

import "musiikkiProjektit/notes"

templ EditNote(note notes.Note){
    <div>
        <label for="editTitle">Title:</label>
        <br/>
        <input id="editTitle" required name="title" type="text" placeholder="Title" value={note.Title}>
        <br/>
        <label for="editNote">Note:</label>
        <br/>
        <textarea id="editNote" required name="note" rows="10" cols="100" placeholder="Note text">{note.Note}</textarea>
    </div>
}
